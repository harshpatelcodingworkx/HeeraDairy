{
   "name": "hiradairy",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "scripts": {
      "start": "ts-node src/index.ts",
      "build": "tsc",
      "dev": "nodemon",
      "migration:generate":"npx typeorm-ts-node-commonjs migration:generate -d src/config/ormconfig.ts",
      "migration:run":"npx typeorm-ts-node-commonjs migration:run -d src/config/ormconfig.ts",
      "migration:revert":"npx typeorm-ts-node-commonjs migration:revert -d src/config/ormconfig.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js" ,
      "typeorm2": "typeorm-ts-node-commonjs -d src/config/ormconfig.ts" 
   },
   "devDependencies": {
      "@types/express": "^5.0.0",
      "@types/node": "^16.18.113",
      "nodemon": "^3.1.7",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "dotenv": "^16.4.5",
      "express": "^4.21.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.20"
   }
}
